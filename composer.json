{
    "name": "alisson-nascimento/cakephp-plugin-shelf",
    "description": "AlissonNascimento's Plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Shelf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shelf\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
