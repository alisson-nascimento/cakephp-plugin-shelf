{
    "name": "alisson-nascimento/cakephp-plugin-shelf",
    "description": "AlissonNascimento's Plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.6",
        "phacil/html": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Shelf\\": "src/"
        },
        "files": [
            "config/helpers.inc.php"
        ]
    },
    "suggest": {
        "alisson-nascimento/cakephp-theme-adminlte": "Caso queira usar a parte de interface web de auditoria"
    }
}
